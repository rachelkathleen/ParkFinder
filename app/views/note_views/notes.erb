<% Note.all.each do |note| %>
<div class="row">
  <div class="col s12 m12">
    <div class="card white">
      <div class="card-content black-text">
        <span class="card-title"><a href="/notes/<%=note.id%>"><%=note.title%></a></span>
          <p><%=note.content%></p>
      </div>
      <div class="card-action">
        <a href="#">Username</a>
        <a href="#">Park name</a>
      </div>
    </div>
  </div>
</div>
<% end %>
