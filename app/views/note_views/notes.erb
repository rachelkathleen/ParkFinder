<% Note.all.each do |note| %>
<div class="row">
   <div class="col s12 m12">
      <div class="card white">
         <div class="card-content black-text grey">
            <span class="card-title"><a href="/notes/<%=note.id%>" class="black-text"><%=note.title%></a></span>
            <p><%=note.content%></p>
         </div>
         <div class="card-action">
            <% if note.user_id %>
            <a href="#" class="black-text"><%=User.find(note.user_id).user_name%></a>
            <% end %>
            <% if note.park_id %>
            <a href="#" class="black-text"><%=Park.find(note.park_id).park_name%></a>
            <% end %>
         </div>
      </div>
   </div>
</div>
<% end %>
