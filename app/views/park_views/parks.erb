<form action="/parks" method="post">
  <input type="text" name="park_name">
  <input class= "button button-primary" type="submit" value="Search">
</form>

<% @parks.each do |park| %>
<div class="row">
   <div class="col s12 m12">
      <div class="card">
         <div class="card-image">
            <img src="<%= park.image%>">
            <span class="card-title"></span>
         </div>
         <div class="card-action" align="center">
            <a style="color:white" class="button button-primary" href="/parks/<%=park.id%>"><%=park.park_name%>, <%=park.state%></a>
         </div>
      </div>
   </div>
</div>
<% end %>
