<p><%= @user.user_name%><p>

  <% @user.notes.each do |note| %>
    <div class="row">
      <div class="col s12 m6">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <span class="card-title"><%=note.title%></span>
            <p><%=note.content%></p>
          </div>
        </div>
      </div>
    </div>
  <%= end %>


